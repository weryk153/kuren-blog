{
    "name": "kuren-blog",
    "version": "1.0.0",
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start",
        "type-check": "tsc",
        "lint": "eslint .",
        "postinstall": "husky install",
        "prepublishOnly": "pinst --disable",
        "postpublish": "pinst --enable",
        "prepare": "husky install"
    },
    "private": false,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*{.tsx,.ts}": [
            "eslint ."
        ]
    },
    "dependencies": {
        "eslint-plugin-react-hooks": "^4.2.0",
        "next": "latest",
        "prettier": "^2.2.1",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "sass": "^1.32.11"
    },
    "devDependencies": {
        "@types/node": "^12.12.21",
        "@types/react": "^17.0.2",
        "@types/react-dom": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "eslint": "^7.24.0",
        "eslint-plugin-react": "^7.23.2",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "pinst": "^2.1.6",
        "typescript": "4.0"
    },
    "license": "MIT"
}
